<?xml version="1.0"?>
<settings>
	<localRepository>/Users/Shared/Development/data/maven/repository/</localRepository>

	<mirrors>
		<mirror>
			<id>nexus-public-snapshots</id>
			<mirrorOf>public-snapshots</mirrorOf>
			<url>http://www1.q2web.de:8081/nexus/content/groups/public-snapshots</url>
		</mirror>
		<mirror>
			<id>nexus</id>
			<mirrorOf>*</mirrorOf>
			<url>http://www1.q2web.de:8081/nexus/content/groups/public</url>
		</mirror>
	</mirrors>

	<profiles>

		<profile>
			<id>defaultProfile</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>

			<repositories>
				<repository>
					<id>maven2-repository.dev.java.net</id>
					<name>Java.net Repository for Maven</name>
					<url>http://download.java.net/maven/2/</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>repository.jboss.com</id>
					<name>JBoss Repository for Maven</name>
					<url>http://repository.jboss.org</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>osgeo</id>
					<name>Open Source Geospatial Foundation</name>
					<url>http://download.osgeo.org/webdav/geotools</url>
				</repository>
				<repository>
					<id>maven.eclipse.org</id>
					<url>http://maven.eclipse.org/nexus/content/groups/public/</url>
				</repository>
				<repository>
					<id>xtend</id>
					<url>http://build.eclipse.org/common/xtend/maven/</url>
				</repository>
				<repository>
					<id>xtext</id>
					<url>http://build.eclipse.org/common/xtext/maven/maven-snapshot/final/</url>
				</repository>
			</repositories>

			<pluginRepositories>
				<pluginRepository>
					<id>xtend</id>
					<url>http://build.eclipse.org/common/xtend/maven/</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>

		<profile>
			<id>database-settings</id>
			<properties>
				<!-- HOSTS -->
				<host.local>localhost</host.local>
				<host.65>192.168.0.65</host.65>
				<host.66>192.168.0.66</host.66>
				<host.69>192.168.0.69</host.69>

				<!-- PORTS -->
				<port.oracle>1521</port.oracle>
				<port.mysql>3306</port.mysql>

				<!-- PROTOCOLS -->
				<protocol.oracle>jdbc:oracle:thin:@//</protocol.oracle>
				<protocol.mysql>jdbc:mysql://</protocol.mysql>

				<!-- URLS -->
				<url.oracle.65>${protocol.oracle}${host.65}/naview01</url.oracle.65>
				<url.oracle.66>${protocol.oracle}${host.66}/naview01</url.oracle.66>
				<url.oracle.69>${protocol.oracle}${host.69}/REFDB01</url.oracle.69>

				<!-- REFPROC -->
				<dev.refproc.development.jdbc.url>${url.oracle.69}</dev.refproc.development.jdbc.url>
				<dev.refproc.development.jdbc.username>refproc</dev.refproc.development.jdbc.username>
				<dev.refproc.development.jdbc.password>refproc</dev.refproc.development.jdbc.password>

				<dev.refproc.local.jdbc.url>${url.oracle.69}</dev.refproc.local.jdbc.url>
				<dev.refproc.local.jdbc.username>refproc</dev.refproc.local.jdbc.username>
				<dev.refproc.local.jdbc.password>refproc</dev.refproc.local.jdbc.password>

			</properties>
		</profile>

		<profile>
			<id>public-snapshots</id>
			<repositories>
				<repository>
					<id>public-snapshots</id>
					<url>http://www1.q2web.de:8081/nexus/content/groups/public-snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>

	</profiles>

	<activeProfiles>
		<activeProfile>defaultProfile</activeProfile>
		<activeProfile>database-settings</activeProfile>
		<activeProfile>public-snapshots</activeProfile>
	</activeProfiles>

</settings>
