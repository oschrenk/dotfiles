{
  "alwaysThinkingEnabled": true,
  "enabledPlugins": {
    "gopls-lsp@claude-plugins-official": true,
    "lua-lsp@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  },
  "env": {
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1"
  },
  "excludePatterns": [
    ".release/**",
    "build/**",
    "dist/**",
    "node_modules/**",
    "out-tsc/**"
  ],
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "AskUserQuestion",
        "hooks": [
          {
            "type": "command",
            "command": "sketchybar --trigger ai_agent_waiting SESSION_ID=$(tmux display-message -p '#{session_id}') WINDOW_ID=$(tmux display-message -p '#{window_id}')"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "AskUserQuestion",
        "hooks": [
          {
            "type": "command",
            "command": "sketchybar --trigger ai_agent_done SESSION_ID=$(tmux display-message -p '#{session_id}') WINDOW_ID=$(tmux display-message -p '#{window_id}')"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ]
  },
  "includeCoAuthoredBy": false,
  "includePatterns": [
    "**/*"
  ],
  "theme": "dark",
  "verbose": false
}
