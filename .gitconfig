[core]
	excludesfile = ~/.global-gitignore
[alias]
	s = status
    d = diff
	dt = difftool
    ci = commit -v
    cia = commit -v -a
    b = branch
	
	history = !git log --reverse --pretty=oneline | cut -c41- | nl | sort -nr
