# put homebrew bin before system bin
export PATH=/usr/local/share/python:/usr/local/bin:/usr/local/sbin:$PATH

# SCRIPTS
export SCRIPTS=~/.scripts
export PATH=$SCRIPTS:$PATH

# INKSCAPE
export PATH=/Applications/Inkscape.app/Contents/Resources/bin:$PATH

# MAVEN
export PATH=~/Applications/Maven/bin:$PATH

# NODE
export NODE_PATH=/usr/local/share/npm/lib/node_modules
export PATH=/usr/local/share/npm/bin:$PATH

# TEX
export PATH=/usr/texbin:$PATH

# CABAL
export PATH=~/.cabal/bin:$PATH

